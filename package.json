        "style-loader": "^2.0.0",
        "tailwindcss": "^2.2.17",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "url-loader": "^4.1.1",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.11.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
