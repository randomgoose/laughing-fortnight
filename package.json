        "@nivo/line": "^0.73.0",
        "@nivo/pie": "^0.73.0",
        "@nivo/radar": "^0.74.0",
        "@nivo/scatterplot": "^0.74.0",
        "@use-gesture/react": "^10.2.0",
        "ahooks": "^2.10.9",
        "antd": "^4.16.11",
        "chroma-js": "^2.4.2",
        "classnames": "^2.3.1",
        "crypto-random-string": "^4.0.0",
        "csv": "^5.5.0",
        "framer-motion": "^4",
        "i18next": "^21.2.4",
